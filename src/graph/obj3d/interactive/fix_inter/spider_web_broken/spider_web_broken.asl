ON INIT {
 COLLISION OFF
 SET_SHADOW OFF
 SET_INTERACTIVITY NONE
 PHYSICAL OFF
 SET_MATERIAL CLOTH
 LOADANIM ACTION1 "spider_web_break"
 PLAYANIM ACTION1
 ACCEPT
}
ON REPLACE {
 REPLACEME SPIDER_WEB
 ACCEPT
}

ON INITEND {
  DESTROY SELF
  ACCEPT
}

ON RELOAD {
  DESTROY SELF
  ACCEPT
}
